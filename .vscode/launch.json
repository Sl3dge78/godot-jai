    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Godot Debug",
            "windows": {
                "program": "D:\\Steam\\steamapps\\common\\Godot Engine\\godot.windows.opt.tools.64.exe",
            },
            "linux": {
                "program": "~/work/godot/bin/godot.linuxbsd.editor.x86_64"
            },
            "args": ["-v", "-e", "example/godot/project.godot"],
            "cwd": "${workspaceFolder}",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Godot",
            "windows": {
                "program": "D:\\Steam\\steamapps\\common\\Godot Engine\\godot.windows.opt.tools.64.exe",
            },
            "linux": {
                "program": "godot"
            },
            "args": ["-v", "-d", "example/godot/project.godot"],
            "cwd": "${workspaceFolder}",
        }
    ]
}

